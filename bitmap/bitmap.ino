/**
 * Dot matrix panel setup
 */
uint8_t rgbPins[] = {25, 26, 27, 14, 12, 13};
uint8_t addrPins[] = {23, 19, 5, 17};
uint8_t clockPin = 16;
uint8_t latchPin = 4;
uint8_t oePin = 15;

Adafruit_Protomatter
    matrix(64,          // Width of matrix (or matrix chain) in pixels
           5,           // Bit depth, 1-6
           1, rgbPins,  // # of matrix chains, array of 6 RGB pins for each
           4, addrPins, // # of address pins (height is inferred), array of pins
           clockPin, latchPin, oePin, // Other matrix control pins
           false); // No double-buffering here (see "doublebuffer" example)

void setup() {
  Serial.begin(115200);

  ProtomatterStatus status = matrix.begin();
  Serial.print("Initializing dot matrix panel. Status: ");
  Serial.println((int)status);
  if (status != PROTOMATTER_OK) {
    while (true)
      ;
  }
  matrix.setRotation(0);

  matrix.drawRGBBitmap(0, 0, tree, 32, 32);
  matrix.setTextSize(1);
  matrix.setCursor(32, 6);
  matrix.setTextColor(matrix.color565(255, 0, 0));
  matrix.print("Merry");

  matrix.setCursor(35, 18);
  // matrix.setTextColor(matrix.color565(0, 16, 0));
  matrix.setTextColor(matrix.color565(0, 255, 0));
  matrix.print("Xmas");
  matrix.show();
}

void loop() {

  
  delay(1000);
}

